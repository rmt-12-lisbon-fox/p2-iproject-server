    "m1": "SET NODE_ENV=test&&sequelize db:migrate --env=test",
    "seedall": "SET NODE_ENV=development&&sequelize db:seed:all",
    "s1": "SET NODE_ENV=test&&sequelize db:seed:all --env=test",
